{
	"compilerOptions": {
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": true,
		"target": "esnext",
		"module": "nodenext",
		"types": [
			"cypress",
			"node"
		],
		"baseUrl": "./",
		"paths": {
			"@/*": [
				"../src/*"
			],
			"@root/*": [
				"../*"
			]
		},
	},
	"include": [
		"**/*.ts",
		"../src/cypress.d.ts"
	]
}
